%section.container
  %h2.page-header Sign in to your account
  %div.well.well-sm
    %div.input-group.form-control
      %label Email Address
      .row
        .col-sm-4
          %input.form-control(type='email')
      %label Password
      .row
        .col-sm-4
          %input.form-control(type='password')
    %br
    %fieldset.form-group.action
      %input{:type => 'submit', :value => 'Sign in', :class => 'btn btn-default'}
